
<!DOCTYPE html>
<html class="no-js" lang="zxx">
<head>
    <meta charset="utf-8">
    <meta http-equiv="x-ua-compatible" content="ie=edge">
    <title>Dima</title>
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <script src="https://cdn.datatables.net/1.13.4/js/jquery.dataTables.min.js"></script>
    <link rel="stylesheet" href="https://cdn.datatables.net/1.13.4/css/jquery.dataTables.min.css">
    <link rel="stylesheet" type="text/css" href="https://cdn.jsdelivr.net/npm/toastify-js/src/toastify.min.css">
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>

    <link rel="icon" href="@Url.Content("~/templates/template/img/logo__dima.png")" />
    @Styles.Render("https://fonts.googleapis.com/icon?family=Material+Icons")

    <!-- CSS here -->
    @Styles.Render("~/templates/template/css/bootstrap.min.css")
    @Styles.Render("~/templates/template/css/owl.carousel.min.css")
    @Styles.Render("~/templates/template/css/magnific-popup.css")
    @Styles.Render("~/templates/template/css/font-awesome.min.css")
    @Styles.Render("~/templates/template/css/themify-icons.css")
    @Styles.Render("~/templates/template/css/nice-select.css")
    @Styles.Render("~/templates/template/css/flaticon.css")
    @Styles.Render("~/templates/template/css/gijgo.css")
    @Styles.Render("~/templates/template/css/animate.css")
    @Styles.Render("~/templates/template/css/slicknav.css")
    @Styles.Render("~/templates/template/css/style.css")
    @Styles.Render("~/templates/template/css/style.map")
    @Styles.Render("~/templates/template/css/theme-default.css")
    @Styles.Render("~/templates/template/css/themify-icons.css")
    @*@Styles.Render("~/templates/template/css/responsive.scss")*@
    @Styles.Render("https://fonts.googleapis.com/css?family=Roboto:300,400&display=swap")
    @Styles.Render("~/templates/login/fonts/icomoon/style.css")
    @Styles.Render("~/templates/login/css/owl.carousel.min.css")

    <!-- Bootstrap CSS -->
    @Styles.Render("~/templates/login/css/bootstrap.min.css")

    <!-- Style -->
    @Styles.Render("~/templates/login/css/style.css")

    @Scripts.Render("~/bundles/modernizr")
</head>
<body>
    <!-- header-start -->
    <header>
        <div class="header-area">
            <div id="sticky-header" class="main-header-area">
                <div class="container">
                    <div class="row align-items-center">
                        <div class="col-xl-3 col-lg-2">
                            <div class="logo">
                                <a href="@Url.Action("Inicial", "Home")" title="List View" class="links">
                                    <img alt="List View" src="@Url.Content("~/templates/template/img/logo__nav.png")">
                                </a>
                            </div>
                        </div>
                        <div class="col-xl-6 col-lg-7">
                            <div class="main-menu d-none d-lg-block">

                                @if (Session["idRol"].ToString() == "1")
                                {
                                    <nav>
                                        <ul id="navigation">
                                            <li>@Html.ActionLink("Inicio", "Inicial", "Home")</li>
                                            <li>
                                                <a href="#">
                                                    Administración
                                                    <i class="ti-angle-down"></i>
                                                    <ul class="submenu">
                                                        <li>@Html.ActionLink("Departmento", "Index", "Departamento")</li>
                                                        <li>@Html.ActionLink("Blog", "ConsultarBlogs", "Blog")</li>
                                                        <li>@Html.ActionLink("Psicólogos", "Index", "Psicologos")</li>
                                                        <li>@Html.ActionLink("Citas", "ConsultarCitas", "Citas")</li>
                                                        <li>@Html.ActionLink("Usuarios", "ConsultarUsuarios", "Home")</li>
                                                    </ul>
                                                </a>
                                            </li>

                                            <li>
                                                <a href="#">
                                                    Información
                                                    <i class="ti-angle-down"></i>
                                                    <ul class="submenu">
                                                        <li>@Html.ActionLink("Nosotros", "Index", "Nosotros")</li>
                                                        <li>@Html.ActionLink("Contacto", "Index", "Contacto")</li>
                                                        <li>@Html.ActionLink("Blog", "Index", "Blog")</li>
                                                    </ul>
                                                </a>
                                            </li>
                                        </ul>
                                    </nav>
                                }


                                @if (Session["idRol"].ToString() == "3")
                                {
                                    <nav>
                                        <ul id="navigation">
                                            <li>@Html.ActionLink("Inicio", "Inicial", "Home")</li>
                                            <li>@Html.ActionLink("Departmento", "Index", "Departamento")</li>
                                            <li>@Html.ActionLink("Citas", "ConsultarCitas", "Citas")</li>
                                            <li>@Html.ActionLink("Blog", "Index", "Blog")</li>
                                            <li>@Html.ActionLink("Nosotros", "Index", "Nosotros")</li>
                                            <li>@Html.ActionLink("Contacto", "Index", "Contacto")</li>
                                        </ul>
                                    </nav>
                                }


                            </div>
                        </div>
                        <div class="col-xl-3 col-lg-3 d-none d-lg-block">
                            @if (Session["idRol"].ToString() == "3")
                            {

                                <div class="Appointment">
                                    <div class="book_btn d-none d-lg-block">
                                        <li>@Html.ActionLink("Agendar Cita", "AgendarCita", "Citas")</li>
                                    </div>
                                </div>
                            }
                            <div class="col-1">
                                <a href="@Url.Action("CerrarSesion","Home")">
                                    <img alt="Cerrar sesion" src="@Url.Content("~/templates/template/img/banner/logout.png")">
                                </a>
                            </div>
                        </div>

                        <div class="col-12">
                            <div class="mobile_menu d-block d-lg-none"></div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </header>
    <!-- header-end -->

    @RenderBody()

    <!-- Emergency_contact start -->
    @if (Session["idRol"].ToString() == "3")

    {
        <div class="Emergency_contact" id="emergency">
            <div class="conatiner-fluid p-0">
                <div class="row no-gutters">
                    <div class="col-xl-6">
                        <div class="single_emergency d-flex align-items-center justify-content-center emergency_bg_1 overlay_skyblue">
                            <div class="info">
                                <h3>Contactos de emergencia</h3>
                                <p>Llámanos para evacuar tus dudas.</p>
                            </div>
                            <div class="info_button">
                                <a href="#emergency-number" class="popup-with-form boxed-btn3-white">+506 2099 9911</a>
                            </div>
                        </div>
                    </div>
                    <div class="col-xl-6">
                        <div class="single_emergency d-flex align-items-center justify-content-center emergency_bg_2 overlay_skyblue">
                            <div class="info">
                                <h3>Agendar una cita en línea</h3>
                                <p>Agenda una cita con nuestros profesionales.</p>
                            </div>
                            <div class="info_button">
                                <a class="boxed-btn3-white" href=@Url.Action("AgendarCita", "Citas")>Agendar Cita</a>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    }
    <!-- Emergency_contact end -->
    <!-- footer start -->
    <footer class="footer">
        @if (Session["idRol"].ToString() == "3")
        {
            <div class="footer_top">
                <div class="container">
                    <div class="row">
                        <div class="col-xl-4 col-md-6 col-lg-4">
                            <div class="footer_widget">
                                <div class="footer_logo">
                                    <a href="#">
                                        <img src="~/templates/template/img/logo__footer.png" alt="">
                                    </a>
                                </div>
                                <p> Estamos para guiar tus pensamientos. </p>
                            </div>
                        </div>
                        <div class="col-xl-2 offset-xl-1 col-md-6 col-lg-3">
                            <div class="footer_widget">
                                <h3 class="footer_title"> Departamentos </h3>
                                <ul>
                                    <li>@Html.ActionLink("Guía mental", "Infantil", "Home")</li>
                                    <li>@Html.ActionLink("Depresión", "Embarazo", "Home")</li>
                                    <li>@Html.ActionLink("Familiar y pareja", "Violencia", "Home")</li>
                                </ul>
                            </div>
                        </div>
                        <div class="col-xl-2 col-md-6 col-lg-2">
                            <div class="footer_widget">
                                <h3 class="footer_title"> Navegación </h3>
                                <ul>
                                    <li>@Html.ActionLink("Nosotros", "Index", "Nosotros")</li>
                                    <li>@Html.ActionLink("Blog", "Index", "Blog")</li>
                                    <li>@Html.ActionLink("Contacto", "Index", "Contacto")</li>
                                    <li>@Html.ActionLink("Cita", "Cita", "Home")</li>
                                </ul>
                            </div>
                        </div>
                        <div class="col-xl-3 col-md-6 col-lg-3">
                            <div class="footer_widget">
                                <h3 class="footer_title"> Dirección </h3>
                                <p>
                                    10109, Rohrmoser, SJ Costa Rica <br>
                                    +506 2099 9911 <br>
                                    ayuda@dima.com
                                </p>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        }
        <div class="copy-right_text">
            <div class="container">
                <div class="footer_border"></div>
                <div class="row">
                    <div class="col-xl-12">
                        <p class="copy_right text-center">
                            Derechos Reservados &copy;
                            <script>document.write(new Date().getFullYear());</script> Desarrollado por Dima
                        </p>
                    </div>
                </div>
            </div>
        </div>
    </footer>
    <!-- footer end  -->
    <!-- link that opens popup -->

    <!-- modal emergency number -->
    <div id="emergency-number" class="white-popup-block mfp-hide">
        <div class="popup_box">
            <div class="popup_inner">
                <h3>Contáctenos en caso de Emergencia</h3>
                <div class="row">
                    <div class="col-xl-12">
                        <h1 id="numero-emergencia">+506 2099 9911</h1>
                    </div>

                    <div class="col-xl-12">
                        <button onclick="copiarNumero()" class="boxed-btn3">Copiar número <i class="ti-layers"></i> </button>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!-- modal emergency number -->
    <!-- JS here -->
    @Scripts.Render("~/templates/login/js/jquery-3.3.1.min.js")
    @Scripts.Render("~/templates/login/js/popper.min.js")
    @Scripts.Render("~/templates/login/js/bootstrap.min.js")
    @Scripts.Render("~/templates/login/js/main.js")
    @Scripts.Render("~/templates/template/js/vendor/modernizr-3.5.0.min.js")
    @Scripts.Render("~/templates/template/js/vendor/jquery-1.12.4.min.js")
    @Scripts.Render("~/templates/template/js/popper.min.js")
    @Scripts.Render("~/templates/template/js/bootstrap.min.js")
    @Scripts.Render("~/templates/template/js/owl.carousel.min.js")
    @Scripts.Render("~/templates/template/js/isotope.pkgd.min.js")
    @Scripts.Render("~/templates/template/js/ajax-form.js")
    @Scripts.Render("~/templates/template/js/waypoints.min.js")
    @Scripts.Render("~/templates/template/js/jquery.counterup.min.js")
    @Scripts.Render("~/templates/template/js/imagesloaded.pkgd.min.js")
    @Scripts.Render("~/templates/template/js/scrollIt.js")
    @Scripts.Render("~/templates/template/js/jquery.scrollUp.min.js")
    @Scripts.Render("~/templates/template/js/wow.min.js")
    @Scripts.Render("~/templates/template/js/nice-select.min.js")
    @Scripts.Render("~/templates/template/js/jquery.slicknav.min.js")
    @Scripts.Render("~/templates/template/js/jquery.magnific-popup.min.js")
    @Scripts.Render("~/templates/template/js/plugins.js")
    @Scripts.Render("~/templates/template/js/gijgo.min.js")
    <!--contact js-->
    @Scripts.Render("~/templates/template/js/contact.js")
    @Scripts.Render("~/templates/template/js/jquery.ajaxchimp.min.js")
    @Scripts.Render("~/templates/template/js/jquery.form.js")
    @Scripts.Render("~/templates/template/js/jquery.validate.min.js")
    @Scripts.Render("~/templates/template/js/mail-script.js")

    @Scripts.Render("~/templates/template/js/main.js")
    <script type="text/javascript" src="https://cdn.jsdelivr.net/npm/toastify-js"></script>
    <script>
        $('#datepicker').datepicker({
            iconsLibrary: 'fontawesome',
            icons: {
                rightIcon: '<span class="fa fa-caret-down"></span>'
            }
        });

        function copiarNumero() {
            var content = document.getElementById('numero-emergencia').innerHTML;
            navigator.clipboard.writeText(content);
        }
    </script>
</body>
</html>